pipeline {
    agent any
    parameters{
      choice(name: 'BUILD_RONDB_VERSION', choices: ['21.04', '22.10'], description: 'RonDB version to build. This is branch in SCM')
    }
    stages {
        stage("build") {
            steps {
              git 'https://github.com/jglick/simple-maven-project-with-tests.git'
              echo "Testing"  
            }
        }
        stage("test") {
            steps {
              echo "Testing"  
            }
        }
        stage("deploy") {
            steps {
              echo "deploying"  
            }
        }
    }   
}

